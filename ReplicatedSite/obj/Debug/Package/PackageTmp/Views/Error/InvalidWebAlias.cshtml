@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "Invalid Web Alias";
    Layout = "~/Views/Shared/_ErrorLayout.cshtml";
}

<div class="jumbotron">
    <div class="container">
        <div class="text-center">
            <h1>@Resources.Common.InvalidWebAlias</h1>
            <p class="lead">
                @Resources.Common.WeCouldNot
            </p>
            <p>
                <a class="btn btn-primary btn-lg" role="button" onclick="history.go(-1);">
                   @Resources.Common.GoBack
                </a>
            </p>
        </div>
    </div>
</div>